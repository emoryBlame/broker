$(function ($) {
	$('.user-content img').each(function () {
		var self = $(this);
		var selfAlt = self.attr('title');
		if (selfAlt !== undefined) {
			$('<div class="img-descr">' + selfAlt + '</div>').insertAfter(self);
		}
	});
});
