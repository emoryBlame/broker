/***********************
 Modal social BEGIN
 ***********************/
var limit = 24 * 3600 * 1000; // 24 часа
var localStorageInitTime = localStorage.getItem('localStorageInitTime');
if (localStorageInitTime === null) {
	localStorage.setItem('localStorageInitTime', +new Date());
} else if (+new Date() - localStorageInitTime > limit){
	localStorage.removeItem("socials_showed");
	localStorage.setItem('localStorageInitTime', +new Date());
}

$(function($){
	var socials_showed = "0";
	window.onscroll = function() {
		socials_showed = localStorage.getItem("socials_showed");
		if ((window.innerHeight + window.scrollY) >= document.body.offsetHeight-50 && socials_showed !== "1") {
			$.fancybox.open(
				{src: '#modal-video'},
				{touch: false}
			);
			localStorage.setItem("socials_showed", "1");
		}
	};
});
/***********************
 Modal social END
 ***********************/
