/***********************
 Calc BEGIN
 ***********************/
function animateValue(id, start, end, duration) {
	var obj = id;
	var range = end - start;
	var minTimer = 50;
	var stepTime = Math.abs(Math.floor(duration / range));
	stepTime = Math.max(stepTime, minTimer);
	var startTime = new Date().getTime();
	var endTime = startTime + duration;
	var timer;

	function run() {
		var now = new Date().getTime();
		var remaining = Math.max((endTime - now) / duration, 0);
		var value = Math.round(end - (remaining * range));
		obj.innerHTML = value;
		if (value === end) {
			clearInterval(timer);
		}
	}

	timer = setInterval(run, stepTime);
	run();
}

$(function ($) {

	var range_sum = $(".calc-range-1");
	var range_date = $(".calc-range-2");

	if (document.documentElement.lang !== "en"){
		range_sum.ionRangeSlider({
			min: 0,
			max: 20000000,
			prettify_separator: ".",
			step: 10000,
			prettify: function (num) {
				if (num > 1000000) {
					return num / 1000000 + " млн.";
				} else {
					return num;
				}
			},
			onFinish: function () {
				calc();
			}
		});
	} else {
		range_sum.ionRangeSlider({
			min: 0,
			max: 1000000,
			prettify_separator: ".",
			step: 1000,
			onFinish: function () {
				calc();
			}
		});
	}


	range_date.ionRangeSlider({
		min: 0,
		max: 540,
		step: 1,
		values: [30, 90, 180, 360, 540],
		onFinish: function () {
			calc();
		}
	});


	var column1 = $('.column--1 .column__body');
	var column2 = $('.column--2 .column__body');
	var percent1 = document.getElementById('percent1');
	var percent2 = document.getElementById('percent2');


	function calc() {
		var sum = range_sum.data('from');
		var date = range_date.data('from');
		var ver1, ver2, dateVer1, dateVer2, sumVer1, sumVer2;

		switch (date) {
			case 0:
				dateVer1 = 18;
				dateVer2 = 94;
				break;
			case 1:
				dateVer1 = 12;
				dateVer2 = 92;
				break;
			case 2:
				dateVer1 = 11;
				dateVer2 = 89;
				break;
			case 3:
				dateVer1 = 8;
				dateVer2 = 88;
				break;
			case 4:
				dateVer1 = 6;
				dateVer2 = 82;
				break;
		}

		if (document.documentElement.lang !== "en"){
			if (sum <= 100000) {
				sumVer1 = 13;
				sumVer2 = 94;
			} else if (sum > 100000 && sum <= 500000) {
				sumVer1 = 11;
				sumVer2 = 93;
			} else if (sum > 500000 && sum <= 1000000) {
				sumVer1 = 7;
				sumVer2 = 89;
			} else if (sum > 1000000) {
				sumVer1 = 6;
				sumVer2 = 87;
			}
		} else {
			if (sum <= 2000) {
				sumVer1 = 13;
				sumVer2 = 94;
			} else if (sum > 2000 && sum <= 8000) {
				sumVer1 = 11;
				sumVer2 = 93;
			} else if (sum > 8000 && sum <= 16000) {
				sumVer1 = 7;
				sumVer2 = 89;
			} else if (sum > 16000) {
				sumVer1 = 6;
				sumVer2 = 87;
			}
		}


		ver1 = (dateVer1 + sumVer1) / 2;
		ver2 = (dateVer2 + sumVer2) / 2;

		// console.log(Math.round(ver1),Math.round(ver2));
		var percent1From = percent1.innerText;
		var percent2From = percent2.innerText;
		animateValue(percent1, percent1From, Math.round(ver1), 600);
		animateValue(percent2, percent2From, Math.round(ver2), 600);

		var column1Height = 360 / 100 * Math.round(ver1);
		var column2Height = 360 / 100 * Math.round(ver2);
		column1.height(column1Height);
		column2.height(column2Height);
	}
});
/***********************
 Calc END
 ***********************/


