/***********************
 Broker check BEGIN
 ***********************/
$(function ($) {

	$.ajaxSetup({cache: false});

	$(".broker-check__form").on("submit", function (event) {
		var send = true;
		event.preventDefault();

		$(this).find("[data-req='true']").each(function () {
			if ($(this).val() === "") {
				$(this).addClass('error');
				send = false;
			}
		});

		$(this).find("[data-req='true']").on('focus click', function () {
			$(this).removeClass('error');
		});

		if (send === true) {
			var userBrokerName = $(this).find('input').val().toLowerCase().replace(/\s+/g, '');

			$.getJSON("/brokers.json", function (data) {
				var goodBrokers = data.good;
				var badBrokers = data.bad;
				var hasMatch = false;

				goodBrokers.forEach(function (elemFromTable) {
					if (userBrokerName === elemFromTable.toLowerCase().replace(/\s+/g, '')) {
						showGoodpanel();
						hasMatch = true;
					}
				});

				badBrokers.forEach(function (elemFromTable) {
					if (userBrokerName === elemFromTable.toLowerCase().replace(/\s+/g, '')) {
						showBadpanel();
						hasMatch = true;
					}
				});

				if (!hasMatch) {
					showNotepanel();
				}
			});
		}
	});
});

function showGoodpanel() {
	$('.broker-check-result--good').addClass('visible');
	$('.broker-check-result--error').removeClass('visible');
	$('.broker-check-result--note').removeClass('visible');
}

function showBadpanel() {
	$('.broker-check-result--error').addClass('visible');
	$('.broker-check-result--good').removeClass('visible');
	$('.broker-check-result--note').removeClass('visible');
}

function showNotepanel() {
	$('.broker-check-result--note').addClass('visible');
	$('.broker-check-result--error').removeClass('visible');
	$('.broker-check-result--good').removeClass('visible');
}
/***********************
 Broker check END
 ***********************/


